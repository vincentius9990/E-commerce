{
  "projects": [
    {
      "name": "frontend",
      "source": "udemy",
      "buildCommand": "npm run build",
      "outputDirectory": "dist",
      "root": "/udemy"
    },
    {
      "name": "backend",
      "source": "backend"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/udemy/build/$1"
    }
  ]
}
